          {
            "scopes": [
              "team",
              "groupchat"
            ],
            "commands": [
              {
                "title": "Check and Install",
                "description": "Display the Welcome Message"
              },
              {
                "title": "Send message",
                "description": "Sends message to all the users"
              }
            ]
          }
        ]
      }
