{
    "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.9/MicrosoftTeams.schema.json",
    "manifestVersion": "1.9",
    "version": "1.0.0",
    "id": "ab1404b9-d8c7-4aa4-8802-f6ae6e057cba",
    "packageName": "com.example.contoso",
    "developer": {
